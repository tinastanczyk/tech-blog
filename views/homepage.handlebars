<div class='card-header col-md-10'>
    <h3>
        Welcome to Tech Blog
    </h3>
</div>
{{#each blogs as |blog|}}
<div class="">
    <div class="card small">
      <div class="card-image">
          <span class="card-title">{{name}}</span>
      </div>
      <div class="card-content">
          <h3>{{title}}</h3>
          <p>{{content}}</p>
      </div>
    </div>
</div>
{{/each}}